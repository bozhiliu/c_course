#include "card.h"

void card_read(FILE* card_file,  DList card_list)
{
	int curr_index = 0;
	while(!feof(card_file))
	{
		char* curr_card[1024];
		char card_char;
		fscanf(card_file, "%c", card_char);
		if(card_char != "\n")
		{
			curr_card[curr_index] = card_char;
			curr_index += 1;
		}
		else
		{
			curr_card[curr_index] = "\n";
			curr_index = 0;
		}

	}


}
