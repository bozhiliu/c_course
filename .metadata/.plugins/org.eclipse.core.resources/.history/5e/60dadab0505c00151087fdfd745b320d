#include "dlist.h"
#include "stdio.h"
#include "card.h"


void main(int argc, char* argv[])
{
	if (argc != 3)
	{
		printf("Usage: chars inputCards inputStrings outputFile\n");
		return;
	}

	FILE* card = fopen(argv[1], "r");
	FILE* str = fopen(argv[2],"r");
	FILE* out = fopen(argv[3], "w");

	DList neg_card ;
	DList neg_str;
	DList pos_str;

	DListInit(neg_card);
	DListInit(neg_str);
	DListInit(pos_str);

	card_read();




}
